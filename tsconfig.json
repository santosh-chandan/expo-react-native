{
  "compilerOptions": {
    "jsx": "react-native",
    "strict": false,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@app/*": ["app/*"]
    }
  },
  "include": [
    "app",
    "src",
    "types",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "tests"
  ],
  "extends": "expo/tsconfig.base"
}
